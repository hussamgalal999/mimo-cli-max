{
  "name": "mimo-cli-max",
  "version": "5.0.0",
  "description": "Production-grade multi-agent CLI platform for autonomous software development",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mimo": "./dist/bin/mimo.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/mimo-cli-max.git"
  },
  "scripts": {
    "mimo": "npx tsx bin/mimo.ts",
    "start": "npm run mimo",
    "build": "tsc",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "ai",
    "multi-agent",
    "autonomous",
    "bmad",
    "productivity"
  ],
  "author": "mimo-cli-max team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@clack/prompts": "^0.11.0",
    "@google/generative-ai": "^0.2.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/diff": "^5.0.9",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "diff": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "inquirer": "^9.2.12",
    "langchain": "^1.1.1",
    "log-update": "^6.1.0",
    "marked": "^11.1.1",
    "marked-terminal": "^7.0.0",
    "openai": "^4.28.0",
    "ora": "^8.0.1",
    "picocolors": "^1.1.1",
    "puppeteer-core": "^24.32.1",
    "vite": "^7.2.7",
